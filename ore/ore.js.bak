			function UpdatePrice ()
			{
				for (var i in Minerals)
				{
					var Material = Minerals[i];
					eval ("var "+Material+"Price = parseFloat(document.getElementById(\""+Material+"Price\").value);");
				}
				
				for (var i in Asteroids)
				{
					var Asteroid = Asteroids[i];
					var Price = 0;
					
					for (var j in Asteroid['Minerals'])
					{
						eval ("var MaterialPrice = "+j+"Price;");
						if (MaterialPrice > 0 && Price >= 0)
						{
							Price += MaterialPrice * Asteroid['Minerals'][j];
						}
						else
						{
							Price = -1;
							break;
						}
					}
					
					if (Price >= 0)
					{
						var PricePerUnit = Math.round(Price / Asteroid['Batchsize']*100)/100;
						var PricePerM3 = Math.round(((Price / Asteroid['Batchsize']) / Asteroid['m3'])*100)/100;
						if (((PricePerUnit*100)%10) == 0)
							{
							//PricePerUnit = PricePerUnit*10;
							}
						if (((PricePerM3*100)%10) == 0)
							{
							//PricePerM3 = PricePerM3*10;
							}
					}
					else
					{
						PricePerUnit = 0.00;
						PricePerM3 = 0.00;
					}
					
					document.getElementById(i+"_unit_price").innerHTML = PricePerUnit;
					document.getElementById(i+"_m3_price").innerHTML = PricePerM3;
					Asteroids[i]["price"] = PricePerUnit;
				}
			}
			
			function UpdateAmt (ore)
			{
				var total_m3 = 0;
				var total_isk = 0;
				var amt = document.getElementById(ore+"_amt").value;
				var isk = Asteroids[ore]["price"]*amt;
				var m3_amt = Asteroids[ore]['m3'] * amt;

				document.getElementById(ore+"_m3").innerHTML = m3_amt.toFixed(2);
				document.getElementById(ore+"_isk").innerHTML = isk.toFixed(2);
				Asteroids[ore]['total_m3'] = m3_amt.toFixed(2);
				Asteroids[ore]['total_isk'] = isk.toFixed(2);

				for (var i in Asteroids)
				{
					total_m3 = eval(total_m3"+"Asteroids[i]['total_m3']);
					total_isk += Asteroids[i]['total_isk'];
				}

				document.getElementById("total_m3").innerHTML = total_m3;
				document.getElementById("total_isk").innerHTML = total_isk;
			}

