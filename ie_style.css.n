/* Portal down*/
#out{
	border-style: solid;
	border-width: 1px;
	border-color: rgb(255, 255, 255);
	background-color: rgb(8, 8, 9);
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10pt;
	color: rgb(255, 0, 0);
	text-align: center;
}

/* Images */
img{
	border: 0px;
}
/* Body */
body{
	background-color: rgb(0, 0, 0);
	background-image: url(./images/style/background.jpg);
	background-repeat: no-repeat;
	background-position: center top;
	background-attachment: fixed;
}

/* Footer */
#panel a{
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 10pt;
	color: #FFA515;
	text-decoration: none;
}

/* Main table*/

table.mainframe{
	width: 800px;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 7px;
	margin: 0;
	border-width: 1px;
	border-style: solid;
	border-color: rgb(255, 255, 255);
	background-color: rgb(8, 8, 9);
}

/* Weblogo */

th.weblogo{
	background-image: url(./images/style/header_bg.gif);
	height: 150px;
	margin: 0;
	background-position: center;
}

th.weblogo img{
	border: 0;
}

/* Main content container */

td.webcontent{
	background-image: url(./images/style/webcontent.png);
	height: 600px;
	vertical-align: top;
	color: rgb(255, 255, 255);
	font-size: 10pt;
	text-decoration: none;
	background-repeat: no-repeat;
	background-attachment: scroll;
	border: 1px;
	border-color: rgb(255, 255, 255);
}

td.webcontent a{
	color: #FFA515;
	text-decoration: none;
	font-size: 10pt;
}
td.webcontent a:hover{
	color: #FFA515;
	text-decoration: underline;
	font-size: 10pt;
}

td.content{
	font-size: 10pt;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	color: rgb(255, 255, 255);
	text-decoration: none;
	vertical-align: top;
	border: 1px;
	border-color: rgb(255, 255, 255);
}

th.content{
	font-size: 10pt;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	color: rgb(255, 255, 255);
	text-decoration: none;
	vertical-align: top;
	border: 1px;
	border-color: rgb(255, 255, 255);
}


/* Copyright message */

td.copyright{
	font-size: 12pt;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	color: rgb(255, 255, 255);
	text-decoration: none;
	vertical-align: top;
	text-align: center;
}

/* Left menu */

*{
	list-style: none;
	margin: 0px;
	padding: 0px;
}

#menu{
	font-size: 12pt;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	color: rgb(255, 255, 255);
	width: 185px;
	border-style: none none none none;

}

#menu li a{
	height: 27px;
	voice-family: "\"}\"";
	voice-family: inherit;
	height: 21px;
	text-decoration: none;
	background-image: url(./images/style/selection.png);
	background-repeat: no-repeat;

}
#menu li a:link, #menu li a:visited{
	font-size: 12pt;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	color: rgb(255, 255, 255);
	display: block;
	padding: 5px 0 0 10px;
}

/* Desplegable menu */

#dmenu{
	font-size: 12pt;
	font-family: Verdana, Arial, Helvetica, sans-serif;
