}

td.webmenu{
	background-image: url(./images/style/menu.png);
	vertical-align: top;
	background-repeat: no-repeat;
	background-color: rgb(77, 77, 87);
	width: 185px;
}

/* News header */

th.header{
	background-image: url(./images/style/news_bg.gif);
	height: 21px;
	display: block;
	height: 21px;
	voice-family: "\"}\"";
	voice-family: inherit;
	height: 21px;
	width: 600px;
}

/* Status info */

#serverstatus{
	font-size: 12pt;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	color: rgb(255, 255, 255);
	text-decoration: none;
	vertical-align: top;
	text-align: center;
}

/* Execution time */

td.querytime{
	font-size: 6pt;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	color: rgb(255, 255, 255);
	text-decoration: none;
	vertical-align: top;
	/* text-align: right; */
	float: right;
}

table.querytime{
	float: right;
}

